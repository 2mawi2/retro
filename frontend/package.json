{
  "name": "@retro/frontend",
  "description": "The React frontend of Retro. Make retrospectives great again.",
  "homepage": "https://github.com/yduman/retro",
  "author": {
    "name": "Yadullah Duman",
    "url": "https://yduman.github.io"
  },
  "bugs": {
    "url": "https://github.com/yduman/retro/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yduman/retro.git",
    "directory": "frontend/"
  },
  "license": "MIT",
  "version": "0.2.1",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:prod": "env-cmd .env.production.local yarn build",
    "deploy:frontend": "cp -a build/. ../backend/public/",
    "deploy": "run-s -l -n build:prod deploy:frontend",
    "test": "react-scripts test --env=jsdom",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "format": "prettier --write \"src/**/*.{js,jsx,css,json}\"",
    "format:check": "prettier --list-different \"src/**/*.{js,jsx,css,json}\"",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "lodash": "^4.17.11",
    "nanoid": "^2.0.1",
    "react": "^16.8.5",
    "react-beautiful-dnd": "^10.1.1",
    "react-dom": "^16.8.5",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.0",
    "recompose": "^0.30.0",
    "socket.io-client": "^2.2.0",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "env-cmd": "^8.0.2",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "jest-dom": "^3.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "react-testing-library": "^6.0.3"
  },
  "proxy": "http://localhost:8081",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
