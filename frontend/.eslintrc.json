{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "jest": true
    },
    "rules": {
        "quotes": [
            "error",
            "double"
        ],
        "react/destructuring-assignment": "off",
        "react/prop-types": "off"
    },
    "overrides": [{
        "files": ["registerServiceWorker.js", "test.js"],
        "rules": {
            "no-console": "off",
            "no-param-reassign": "off",
            "no-use-before-define": "off",
            "import/no-extraneous-dependencies": "off"
        }
    }]
}